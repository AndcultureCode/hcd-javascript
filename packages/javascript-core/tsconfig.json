{
  "extends": "@rsm-hcd/tsconfig/base.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["dom", "dom.iterable", "ES2015"],
    "module": "commonjs",
    "noUnusedParameters": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "target": "es5",
    "outDir": "dist"
  },
  "include": ["src"]
}
